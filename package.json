{
  "name": "ant-design-pro",
  "version": "3.10.7",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "build": "cross-env NO_PROXY=true ESLINT=none roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@antv/data-set": "0.8.0",
    "@babel/polyfill": "7.0.0-beta.36",
    "@babel/runtime": "7.0.0-beta.46",
    "antd": "3.8.4",
    "babel-runtime": "6.9.2",
    "bizcharts": "3.2.2",
    "bizcharts-plugin-slider": "2.0.7",
    "braft-editor": "2.2.9",
    "classnames": "2.2.5",
    "dva": "2.4.1",
    "dva-loading": "1.0.4",
    "echarts": "4.1.0",
    "echarts-for-react": "2.0.14",
    "enquire-js": "0.2.1",
    "history": "4.7.2",
    "js-cookie": "2.2.0",
    "js-yaml": "^3.13.1",
    "json2yaml": "1.1.0",
    "lodash": "4.17.19",
    "lodash-decorators": "4.4.1",
    "moment": "2.19.3",
    "nprogress": "0.2.0",
    "numeral": "2.0.6",
    "omit.js": "1.0.0",
    "path-to-regexp": "2.4.0",
    "prop-types": "15.5.10",
    "qs": "6.5.0",
    "rc-drawer-menu": "0.5.0",
    "react": "16.4.2",
    "react-color": "2.14.1",
    "react-container-query": "0.11.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-document-title": "2.0.3",
    "react-dom": "16.4.2",
    "react-fittext": "1.0.0",
    "react-redux": "6.0.0",
    "react-syntax-highlighter": "8.1.0",
    "react-websocket": "2.0.0",
    "rollbar": "2.4.5",
    "sockjs-client": "1.3.0",
    "umi": "2.0.3",
    "url-polyfill": "1.0.14",
    "xterm": "3.8.1",
    "yaml2json": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-plugin-dva-hmr": "0.4.1",
    "babel-plugin-import": "1.6.7",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "cross-env": "5.1.1",
    "cross-port-killer": "1.0.1",
    "enzyme": "3.5.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-prettier": "2.10.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-compat": "2.5.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-markdown": "1.0.0-beta.8",
    "eslint-plugin-react": "7.11.1",
    "gh-pages": "1.0.0",
    "history": "4.7.2",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "mockjs": "1.0.1-beta3",
    "prettier": "1.11.1",
    "pro-download": "1.0.1",
    "redbox-react": "1.5.0",
    "regenerator-runtime": "0.11.1",
    "roadhog": "2.4.5",
    "roadhog-api-doc": "0.3.4",
    "stylelint": "8.4.0",
    "stylelint-config-prettier": "3.0.4",
    "stylelint-config-standard": "18.0.0",
    "webpack-cli": "3.2.1"
  },
  "optionalDependencies": {
    "puppeteer": "1.1.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --wirter",
      "git add"
    ],
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "theme": {
    "primary-color": "#d4380d"
  }
}
